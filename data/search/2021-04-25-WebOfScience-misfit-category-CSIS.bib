}

@inproceedings{ ISI:000076408400178,
Author = {Bulow, KG and Callies, U and Rhodin, A},
Editor = {{Babovic, V and Larsen, LC}},
Title = {{Sensitivity studies using a nutrient model for the River Elbe}},
Booktitle = {{HYDROINFORMATICS `98, VOLS 1 AND 2}},
Year = {{1998}},
Pages = {{1299-1304}},
Note = {{3rd International Conference on Hydroinformatics, COPENHAGEN, DENMARK,
   AUG 24-26, 1998}},
Organization = {{Int Assoc Hydraul Res; Danish Hydraul Inst; UNESCO; Int Org Water
   Quality; Amer Soc Civil Engineers; World Meteorol Org; Danish Acad
   Engineers; Danish Water Resources Comm}},
Abstract = {{The sensitivities of a simple zero-dimensional model which describes the
   major nutrient processes and the oxygen concentration in the tidal
   influenced Elbe estuary were studied. For this purpose the Integrated
   Modeling and Analysis System (IMAS) was used. By using the adjoint
   model, the impact of the initial model parameters on their counterparts
   after a period of ten days were studied. By doing this it was
   determined, which model compartment is acting on the other ones and
   which is not doing so. By minimizing the misfit between the
   observational data and their model counterparts (quadratic cost
   function), it is possible to estimate the the optimal values of the
   parameters used in the model. The Hessian matrix of the cost function
   offers the opportunity to determine the posteriori uncertainty of the
   model parameters. Analyzing the eigenmodes of the Hessian yields those
   linear combinations of the model parameters, which are most effectively
   controlled by data, i.e. for which the posteriori confidence interval is
   significantly smaller than the prior confidence interval.}},
ISBN = {{90-5410-983-1}},
Unique-ID = {{ISI:000076408400178}},
}

