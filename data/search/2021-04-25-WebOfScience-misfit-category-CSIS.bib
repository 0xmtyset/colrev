@article{ ISI:A1991FR14000004,
Author = {OLERUP, A},
Title = {{DESIGN APPROACHES - A COMPARATIVE-STUDY OF INFORMATION-SYSTEM DESIGN AND
   ARCHITECTURAL DESIGN}},
Journal = {{COMPUTER JOURNAL}},
Year = {{1991}},
Volume = {{34}},
Number = {{3}},
Pages = {{215-224}},
Month = {{JUN}},
Abstract = {{In this paper two approaches to design are compared, namely those
   proposed by Langefors and Alexander.  The Langeforsian approach aims to
   introduce effective methods for information system design and to utilise
   the potential of computers and information technology in controlling
   organisations and improving their performance.  The aim of Alexander's
   approach is to improve architectural design by bridging the gap between
   designer and users.  Both approaches have given important contributions
   to design in their fields.  In both approaches graphs and matrices are
   important tools.  Also, procedures for finding subsystems and components
   are put forward in both approaches, and the process of design is
   subdivided.  The two approaches are different with regard to
   requirements definitions, in the Langeforsian approach goals are
   crucial, while misfits are focussed in Alexander's approach.  The
   notions of goals and misfits are examined.  Finally, systems-fit
   approaches to information system design are discussed.}},
DOI = {{10.1093/comjnl/34.3.215}},
ISSN = {{0010-4620}},
Unique-ID = {{ISI:A1991FR14000004}},
}

@article{ ISI:A1990LF39500096,
Author = {ABASTANOTTI, D and GEROMEL, JC and GHIDONI, R and GUARDABASSI, G},
Title = {{OPTIMIZATION-BASED NYCELY NONLINEAR MODELING}},
Journal = {{LECTURE NOTES IN CONTROL AND INFORMATION SCIENCES}},
Year = {{1990}},
Volume = {{143}},
Pages = {{885-893}},
Abstract = {{A system M tangent to a plant P at the nominal equilibrium is a nicely
   nonlinear model of P if it is rigorously linearizable by output
   feedback. The general structure of a nicely nonlinear modelling problem
   can be outlined as follows. Given: i) a class of parametrized nonlinear
   models, and a feasible parameter set, such that any feasible model is
   nicely nonlinear, ii) a family of significant operating conditions
   (input-output pairs) of P, minimize, over the feasible parameter set, a
   nonnegative misfit function (relative to the considered family of
   operating conditions) between the nicely nonlinear model and the plant
   The purpose of the present paper is to present an algorithm for the
   semi-infinite nonlinear programming problem resulting from a particular
   instance of the general setting above.}},
ISSN = {{0170-8643}},
ResearcherID-Numbers = {{Geromel, Jose C/E-3103-2012}},
Unique-ID = {{ISI:A1990LF39500096}},
}
