}

@article{ ISI:000320720900008,
Author = {Torres, S. O. A. and Mesquita Filho, A. C. and Miranda, P. E. V.},
Title = {{Control Strategy for Balance of Plant in Solid Oxide Fuel Cell}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2013}},
Volume = {{11}},
Number = {{2}},
Pages = {{726-736}},
Month = {{MAR}},
Abstract = {{This paper presents a control strategy for a power generation plant
   based in solid oxide fuel cells. The strategy establishes a stationary
   regime of operation for the fuel cell using a small battery bank to
   absorb the dynamic characteristics of the power demand. In order to make
   the control strategy the methods of genetic algorithms was used as
   optimization tool for determining the optimal point in the stationary
   regime. With the control strategy adopted is achieved a bigger
   efficiency of the equipment due to the absence of misfits in the
   operation parameters. This methodology also promotes an increase in life
   cycle and reduction the volume and cost of equipment due to operation at
   the point of maximum efficiency. A simulation structure was made to test
   the solutions developed and proposed configuration. The capability of
   dynamic response was evaluated with this structure introducing
   operations conditions based on a residential power demand.}},
DOI = {{10.1109/TLA.2013.6533961}},
ISSN = {{1548-0992}},
Unique-ID = {{ISI:000320720900008}},
}

@article{ ISI:000308254200020,
Author = {Smith, Richard D. and Engdahl, Alaina L. and Dunbar, Jr., James B. and
   Carlson, Heather A.},
Title = {{Biophysical Limits of Protein-Ligand Binding}},
Journal = {{JOURNAL OF CHEMICAL INFORMATION AND MODELING}},
Year = {{2012}},
Volume = {{52}},
Number = {{8}},
Pages = {{2098-2106}},
Month = {{AUG}},
Abstract = {{In classic work, Kuntz et al. (Proc. Nat. Acad. Sci. USA 1999, 96,
   9997-10002) introduced the concept of ligand efficiency. Though that
   study. focused primarily on drug-like molecules, it also showed that
   Metal binding led to the greatest ligand efficiencies. Here, the
   physical limits of binding are examined across the wide variety of small
   molecules in the Binding MOAD database. The complexes with the greatest
   ligand efficiencies share the trait of being small, charged ligands
   bound in highly charged, well buried binding sites. The limit of ligand
   efficiency is -1.75 kcal/mol.atom for the protein-ligand complexes
   within Binding MOAD and 95\% of the set have efficiencies below a ``soft
   limit{''} of -0.83 kcal/mol.atom. On the basis of buried molecular
   surface area, the hard limit of ligand efficiency is -117
   cal/mol.angstrom(2), which is in surprising agreement with the limit of
   macromolecule protein binding. Close examination of the most efficient
   systems reveals their incredibly high efficiency is dictated by tight
   contacts, between the charged groups Of the ligand and the pocket. In
   fact, a Misfit of 0.24 angstrom in the average contacts inherently
   decreases the maximum possible efficiency by at least 0.1 kcal/mol.atom.}},
DOI = {{10.1021/ci200612f}},
ISSN = {{1549-9596}},
EISSN = {{1549-960X}},
ResearcherID-Numbers = {{Smith, Richard D/J-3664-2012
   }},
ORCID-Numbers = {{Smith, Richard D/0000-0002-2381-2349
   Carlson, Heather/0000-0002-7495-1699}},
